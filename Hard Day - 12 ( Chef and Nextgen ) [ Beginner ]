#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t ;
	cin>>t ;
	while(t--)
	{
	    int A , B , X ,Y ;
	    cin>>A>>B>>X>>Y ;
	    int earth ;
	    earth = A*B ;
	    int moon = X*Y ;
	    if(earth<=moon)
	    {
	        cout<<"YES"<<endl ;
	    }
	    else
	    {
	        cout<<"NO"<<endl ;
	    }
	}
	return 0;
}
